    "transpile": "enyo-transpile",
    "pack": "enyo-webpack",
    "pack-p": "enyo-webpack -p",
    "watch": "enyo-webpack --watch",
    "link-enyo": "npm link enyo-core enyo-ui",
    "clean": "enyo-clean"
