{
  "name": "enyo-ui",
  "version": "0.1.0",
  "description": "A collection of simplified unstyled cross-platform UI components for Enyo",
  "main": "index.js",
  "scripts": {
    "test": "enyo-karma start --single-run --browsers PhantomJS",
    "test-json": "enyo-karma start --single-run --browsers PhantomJS --reporters json",
    "test-watch": "enyo-karma start --browsers PhantomJS",
    "transpile": "enyo-transpile",
    "pack": "enyo-webpack",
    "pack-p": "enyo-webpack -p",
    "watch": "enyo-webpack --watch",
    "link-enyo": "npm link enyo-core",
    "clean": "enyo-clean"
  },
  "homepage": "http://enyojs.com/",
  "bugs": "http://jira.enyojs.com/",
  "maintainers": [
    {
      "name": "Enyo JS Framework Team",
      "web": "http://enyojs.com/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enyojs/enyo-ui.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "framework",
    "toolkit",
    "components",
    "mobile",
    "webOS"
  ],
  "devDependencies": {
    "enyo-config": "enyojs/enyo-config#test-suite"
  },
  "dependencies": {
    "enyo-core": "enyojs/enyo-core",
    "recompose": "^0.20.2"
  }
}
