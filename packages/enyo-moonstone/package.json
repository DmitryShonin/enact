  },
  "dependencies": {
    "eases": "^1.0.8",
    "enyo-core": "enyojs/enyo-core#develop",
    "enyo-i18n": "github:enyojs/enyo-i18n#develop",
    "enyo-spotlight": "enyojs/enyo-spotlight#develop",
